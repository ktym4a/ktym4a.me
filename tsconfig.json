{
    "extends": "astro/tsconfigs/strict",
    "importsNotUsedAsValues": "error",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/assets/*": ["src/assets/*"],
            "@/components/*": ["src/components/*"],
            "@/layouts/*": ["src/layouts/*"],
            "@/types/*": ["src/types/*"]
        }
    }
}
