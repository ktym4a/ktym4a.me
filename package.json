        "@astrojs/sitemap": "^3.1.1",
        "@astrojs/tailwind": "^5.1.0",
        "@catppuccin/tailwindcss": "^0.1.6",
        "@typescript-eslint/parser": "^7.2.0",
        "astro-capo": "^0.0.1",
